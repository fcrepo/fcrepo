        <Set name="loginService">
            <New class="org.eclipse.jetty.security.HashLoginService">
                <Set name="name">fcrepo</Set>
                <Set name="config">src/test/resources/jetty-users.properties</Set>
            </New>
        </Set>
    </Get>
</Configure>
