<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="owl2html.xsl"?>

<rdf:RDF
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="static/constraints/InvalidMementoPathException">
        <rdfs:label xml:lang="en">Invalid Memento Path Exception</rdfs:label>
        <rdfs:comment xml:lang="en">
          Fedora cannot handle versioning requests with a malformed memento ID.
          Memento ID is system generated, which is a timestamp in yyyyMMddHHmmss format.
        </rdfs:comment>
    </owl:Ontology>
</rdf:RDF>
