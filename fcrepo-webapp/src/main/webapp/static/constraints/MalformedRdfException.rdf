<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="owl2html.xsl"?>

<rdf:RDF
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="static/constraints/MalformedRdfException">
        <rdfs:label xml:lang="en">Malformed RDF Exception</rdfs:label>
        <rdfs:comment xml:lang="en">The RDF was incorrectly formed.  The response body from the failing request
        should offer useful information regarding the failure.</rdfs:comment>
    </owl:Ontology>
</rdf:RDF>
