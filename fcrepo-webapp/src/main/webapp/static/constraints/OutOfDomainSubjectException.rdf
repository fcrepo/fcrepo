<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="static/constraints/OutOfDomainSubjectException">
        <rdfs:label xml:lang="en">Out Of Domain Subject Exception</rdfs:label>
        <rdfs:comment xml:lang="en">The RDF contained one or more triples where the subject was not contained inside
        the Fedora Repository. Fedora does not maintain RDF content about resources not contained within it.</rdfs:comment>
    </owl:Ontology>
</rdf:RDF>